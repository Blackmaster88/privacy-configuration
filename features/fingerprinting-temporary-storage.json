{
    "_meta": {
        "description": "Fingerprintable storage protection",
        "sampleExcludeRecords": {
            "domain": "example.com",
            "reason": "site breakage"
        }
    },
    "exceptions": [],
    "_patches": {
        "android": [
            {
                "op": "replace",
                "path": "/state",
                "value": "enabled"
            },
            {
                "op": "add",
                "path": "/exceptions/-",
                "value": {
                    "domain": "ticketmaster.com",
                    "reason": "https://github.com/duckduckgo/privacy-configuration/issues/575"
                }
            }
        ],
        "extension": [
            {
                "op": "replace",
                "path": "/state",
                "value": "enabled"
            }
        ],
        "ios": [
            {
                "op": "replace",
                "path": "/state",
                "value": "enabled"
            }
        ]
    }
}