{
    "_meta": {
        "description": "Rewrite links with tracking parameters to safer alternatives.",
        "sampleExcludeRecords": {
            "domain": "example.com",
            "reason": "site breakage"
        }
    },
    "exceptions": [],
    "settings": {
        "parameters": [
            "utm_source",
            "utm_medium",
            "utm_campaign",
            "utm_term",
            "utm_content",
            "gclid",
            "fbclid",
            "fb_action_ids",
            "fb_action_types",
            "fb_source",
            "fb_ref",
            "ga_source",
            "ga_medium",
            "ga_term",
            "ga_content",
            "ga_campaign",
            "ga_place",
            "action_object_map",
            "action_type_map",
            "action_ref_map",
            "gs_l",
            "mkt_tok",
            "hmb_campaign",
            "hmb_source",
            "hmb_medium"
        ]
    },
    "_patches": {
        "android": [
            {
                "op": "replace",
                "path": "/state",
                "value": "enabled"
            },
            {
                "op": "add",
                "path": "/minSupportedVersion",
                "value": 51250000
            }
        ],
        "extension-brave": [
            {
                "op": "remove",
                "path": "/state"
            }
        ],
        "extension-bravemv3": [
            {
                "op": "remove",
                "path": "/state"
            }
        ],
        "extension-chrome": [
            {
                "op": "remove",
                "path": "/state"
            }
        ],
        "extension-chromemv3": [
            {
                "op": "remove",
                "path": "/state"
            }
        ],
        "extension": [
            {
                "op": "replace",
                "path": "/state",
                "value": "enabled"
            }
        ],
        "extension-edg": [
            {
                "op": "remove",
                "path": "/state"
            }
        ],
        "extension-edge": [
            {
                "op": "remove",
                "path": "/state"
            }
        ],
        "extension-edgmv3": [
            {
                "op": "remove",
                "path": "/state"
            }
        ],
        "extension-firefox": [
            {
                "op": "remove",
                "path": "/state"
            }
        ],
        "extension-safarimv3": [
            {
                "op": "remove",
                "path": "/state"
            }
        ],
        "ios": [
            {
                "op": "replace",
                "path": "/state",
                "value": "enabled"
            }
        ],
        "macos": [
            {
                "op": "replace",
                "path": "/state",
                "value": "enabled"
            },
            {
                "op": "add",
                "path": "/minSupportedVersion",
                "value": "0.22.3"
            }
        ],
        "windows": [
            {
                "op": "remove",
                "path": "/state"
            }
        ]
    }
}