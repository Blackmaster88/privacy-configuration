{
    "_meta": {
        "description": "Display permission usage in the Windows app",
        "sampleExcludeRecords": {
            "domain": "example.com",
            "reason": "site breakage"
        }
    },
    "exceptions": [],
    "_patches": {
        "android": [
            {
                "op": "remove",
                "path": "/state"
            }
        ],
        "extension": [
            {
                "op": "remove",
                "path": "/state"
            }
        ],
        "ios": [
            {
                "op": "remove",
                "path": "/state"
            }
        ],
        "macos": [
            {
                "op": "remove",
                "path": "/state"
            }
        ],
        "windows": [
            {
                "op": "replace",
                "path": "/state",
                "value": "enabled"
            }
        ]
    }
}