{
    "state": "disabled",
    "exceptions": [
        {
            "domain": "open.spotify.com",
            "reason": "Site displays a 'Playback disabled' notice on some media, preventing listening."
        },
        {
            "domain": "plex.tv",
            "reason": "https://github.com/duckduckgo/privacy-configuration/issues/684"
        }
    ],
    "_patches": {
        "android": [
            {
                "op": "remove",
                "path": "/_patches"
            },
            {
                "op": "replace",
                "path": "/state",
                "value": "enabled"
            }
        ],
        "extension": [
            {
                "op": "remove",
                "path": "/_patches"
            }
        ],
        "ios": [
            {
                "op": "remove",
                "path": "/_patches"
            }
        ],
        "macos": [
            {
                "op": "remove",
                "path": "/_patches"
            }
        ],
        "windows": [
            {
                "op": "remove",
                "path": "/_patches"
            }
        ]
    }
}